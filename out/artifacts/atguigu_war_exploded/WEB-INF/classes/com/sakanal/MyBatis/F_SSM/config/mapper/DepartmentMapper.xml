<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sakanal.MyBatis.F_SSM.dao.DepartmentMapper">
    <select id="getDepartmentById" resultType="com.sakanal.MyBatis.F_SSM.pojo.Department">
        select id,department_name name from `department` where id=#{id}
    </select>
</mapper>