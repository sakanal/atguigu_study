<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sakanal.mvc.demo2.dao.UserMapper">
    <select id="getUserAll" resultType="com.sakanal.mvc.demo2.pojo.User">
        select * from `user`
    </select>

    <insert id="insertUser">
        insert into `user`(name,password,gender,address,phone)
        value (#{name},#{password},#{gender},#{address},#{phone})
    </insert>
</mapper>